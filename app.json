{
  "expo": {
    "name": "organisr",
    "slug": "organisr",
    "version": "1.0.0",
    "icon": "./assets/images/app-icon@2x.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "orientation": "portrait",
    "githubUrl": "https://github.com/codingkatty/organisr",
    "ios": {
      "supportsTablet": true,
      "requiresFullScreen": true,
      "infoPlist": {
        "UISupportedInterfaceOrientations~ipad": [
          "UIInterfaceOrientationPortrait",
          "UIInterfaceOrientationPortraitUpsideDown",
          "UIInterfaceOrientationPortrait",
          "UIInterfaceOrientationPortraitUpsideDown"
        ],
        "ITSAppUsesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.candy.organisr"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/app-icon-2d.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-screen-orientation",
        {
          "initialOrientation": "PORTRAIT_UP"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "This app accesses your photos to let you choose images for your notes.",
          "cameraPermission": "This app accesses your camera to let you take pictures for your notes."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "3120da4c-6790-4508-950d-7b6f60059d34"
      }
    }
  }
}
